import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String str=in.next();
        LinkedHashSet<Character>set=new LinkedHashSet<>();
        for (int i=0; i<str.length();i++){
            char c = str.charAt(i);
            if (!set.contains(c)){
                set.add(c);
            }
        }       
        for (char c : set){
            System.out.print(c);
        }            
    }        
}
